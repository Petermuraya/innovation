
# Copy this file to .env.local and fill in your actual values
# .env.local is gitignored and should contain your real secrets

# ===========================================
# REQUIRED: Supabase Configuration
# ===========================================
# 1. Go to https://app.supabase.com
# 2. Create a new project or select existing one
# 3. Go to Settings > API
# 4. Copy the values below

VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# ===========================================
# REQUIRED: Site Configuration
# ===========================================
VITE_BASE_URL=http://localhost:5173
VITE_SITE_NAME=Karatina Innovation Club
VITE_CONTACT_EMAIL=info@karatinatech.com

# ===========================================
# OPTIONAL: Analytics & SEO
# ===========================================
# Google Analytics 4 Measurement ID
# Get from: https://analytics.google.com
VITE_GA_TRACKING_ID=

# Google Search Console verification
VITE_GOOGLE_SITE_VERIFICATION=

# ===========================================
# OPTIONAL: Social Media Integration
# ===========================================
VITE_TWITTER_HANDLE=@KaratinaTech
VITE_GITHUB_ORG=karatina-innovation-club
VITE_LINKEDIN_COMPANY=karatina-innovation-club
VITE_YOUTUBE_CHANNEL=

# ===========================================
# OPTIONAL: Third-party Services
# ===========================================
# Stripe (for international payments)
VITE_STRIPE_PUBLISHABLE_KEY=

# SendGrid (for email notifications)
VITE_SENDGRID_API_KEY=

# Cloudinary (for image optimization)
VITE_CLOUDINARY_CLOUD_NAME=
VITE_CLOUDINARY_API_KEY=

# ===========================================
# DEVELOPMENT SETTINGS
# ===========================================
VITE_NODE_ENV=development
VITE_DEBUG_MODE=true
VITE_API_TIMEOUT=10000

# ===========================================
# PRODUCTION SETTINGS (for production .env.local)
# ===========================================
# VITE_NODE_ENV=production
# VITE_DEBUG_MODE=false
# VITE_BASE_URL=https://karatinatech.com
